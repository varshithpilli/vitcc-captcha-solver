{
  "manifest_version": 3,
  "name": "Lazy VITC",
  "version": "1.1",
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "sidePanel"
  ],
  "host_permissions": [
    "*://vtopcc.vit.ac.in/*",
    "*://vtopregcc.vit.ac.in/*",
    "*://vtop.vit.ac.in/*",
    "*://lms.vit.ac.in/*"
  ],
  "side_panel": {
    "default_path": "sidebar/sidebar.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "weights.json",
        "sidebar/sidebar.js",
        "sidebar/facultyRatings.js",
        "sidebar/ffcs.js",
        "sidebar/ffcs2-multiWrapper.js",
        "sidebar/example.js",
        "sidebar/credentialsInjection.js",
        "sidebar/vtop_calendar_extractor.js",
        "navigation/facultyRatingsInjector.js"
      ],
      "matches": [
        "*://vtopcc.vit.ac.in/*",
        "*://vtopregcc.vit.ac.in/*",
        "*://vtop.vit.ac.in/*",
        "*://lms.vit.ac.in/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "*://vtopcc.vit.ac.in/*",
        "*://vtopregcc.vit.ac.in/*",
        "*://vtop.vit.ac.in/*",
        "*://lms.vit.ac.in/*"
      ],
      "js": [
        "login/utils.js",
        "login/captcha.js",
        "login/autoLogin.js",
        "navigation/attendance.js",
        "navigation/marks.js",
        "navigation/nav.js",
        "navigation/detector.js",
        "navigation/facultyRatingsInjector.js",
        "sidebar/ffcs.js",
        "sidebar/ffcs2-multiWrapper.js",
        "sidebar/vtop_calendar_extractor.js"
      ]
    }
  ]
}